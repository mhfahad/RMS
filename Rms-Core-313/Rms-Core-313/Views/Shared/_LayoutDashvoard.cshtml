
<html lang="en">
<head>
    <title>Sidebar 03</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800,900" rel="stylesheet">
    <script src="~/Scripts/jquery-3.3.1.js"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">

    <link href="~/Tem2/css/style.css" rel="stylesheet" />
</head>
<body>
    <div class="wrapper d-flex align-items-stretch">
        <nav id="sidebar" class="active" style="background-color:dimgray">
            <div class="custom-menu">
                <button type="button" id="sidebarCollapse" class="btn btn-primary">
                    <i class="fa fa-bars"></i>
                    <span class="sr-only">Toggle Menu</span>
                </button>
            </div>
            <div class="p-4">
                <h1><a href="~/MenuCreate/Create" class="logo">Dashboard</a></h1>
                <ul class="list-unstyled components mb-5">
                    <li class="active">
                        <a href="~/Home/Index"><span class="fa fa-home mr-3"></span> Home</a>
                    </li>
                    <li>
                        <a href="~/MenuCreate/CreateType"><span class="fa fa-user mr-3"></span>Create Food Type</a>
                    </li>
                    <li>
                        <a href="~/MenuCreate/Create"><span class="fa fa-briefcase mr-3"></span> Create Menus</a>
                    </li>
                    <li>
                        <a href="~/MenuCreate/GetMenuList"><span class="fa fa-briefcase mr-3"></span>Menu List</a>
                    </li>
                    <li>
                        <a href="~/TableBooking/GetBookingList"><span class="fa fa-sticky-note mr-3"></span> Booking Map</a>
                    </li>
                    <li>
                        <a href="~/OnlineOrder/GetOrderList"><span class="fa fa-sticky-note mr-3"></span> Order Map</a>
                    </li>
                    
                </ul>

               
                 
            </div>
        </nav>
        <!-- Page Content  -->
        <div id="content" class="p-4 p-md-5 pt-5 container">
            @RenderBody()
        </div>
    </div>
    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)


    <script src="~/Scripts/Template/jquery/jquery.min.js"></script>
    <script src="~/Scripts/Template/bootstrap/bootstrap.min.js"></script>
    <script src="~/Tem2/js/main.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAaXCxqnCwD2cQ4RE_WuFfW0ECKkYBelhU&sensor=true"></script>
    <script type="text/javascript" src="~/Scripts/Template/jquery/jquery.min.js"></script>
    <script type="text/javascript" src="~/scripts/Template/jquery/jquery.mb.YTPlayer.js"></script>
   
    <script src="~/Scripts/Template/jquery/jquery.parallax-1.1.3.js"></script>
    <script src="~/Scripts/Template/jquery/jquery.flexslider-min.js"></script>
  
    <script type="text/javascript" src="~/scripts/template/jquery.countTo.js"></script>
    <script type="text/javascript" src="~/scripts/template/jquery.scrollTo-min.js"></script>
    <script type="text/javascript" src="~/scripts/template/jquery.magnific-popup.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/momentjs/2.14.1/moment.min.js"></script>
</body>
</html>